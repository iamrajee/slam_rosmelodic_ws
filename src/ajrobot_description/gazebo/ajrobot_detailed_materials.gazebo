</gazebo>

<gazebo reference="tubebl_1">
  <material>${blueglow_color}</material>
</gazebo>

<gazebo reference="tubebr_1">
  <material>${blueglow_color}</material>
</gazebo>

<gazebo reference="tubefl_1">
  <material>${blueglow_color}</material>
</gazebo>

<gazebo reference="lidar_1">
  <material>${blue_color}</material>
</gazebo>

<gazebo reference="camera_1">
  <material>${flatblack_color}</material>
</gazebo>

<gazebo reference="pir_1">
  <material>${blue_color}</material>
</gazebo>

</robot>
